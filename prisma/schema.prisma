generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model Projeto {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  tecnologias  String[]
  nome         String
  descricao    String
  data_criacao DateTime @default(now())
  linkDemo     String
  linkRepo     String
  status       Boolean

  @@map("projetos")
}
