generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Projeto {
  id           String   @id @default(uuid())
  tecnologias  String[]
  nome         String
  descricao    String
  data_criacao DateTime @default(now())
  linkDemo     String
  linkRepo     String
  status       Boolean
}
